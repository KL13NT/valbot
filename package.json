{
	"name": "valbot",
	"version": "1.0.0",
	"description": "A bot for valarium server!",
	"main": "./prod/index.js",
	"repository": "git@github.com:KL13NT/valbot.git",
	"author": "KL13NT <Nabil.tharwat@outlook.com>",
	"license": "GPL-3.0-only",
	"scripts": {
		"watch": "tsc --watch",
		"inspect": "yarn run-p develop:mongo develop:redis watch inspect:node",
		"develop": "yarn run-p develop:mongo develop:redis watch develop:node",
		"develop:mongo": "mongod --dbpath=data",
		"develop:redis": "redis-server",
		"start": "tsc && node ./prod/index.js",
		"develop:node": "nodemon -r dotenv/config ./prod/index.js --inspect",
		"inspect:node": "nodemon --inspect -r dotenv/config ./prod/index.js --inspect",
		"lint": "eslint --ext .js,.ts ./src --config .eslintrc.js --fix",
		"pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
		"test": "jest --colors --expand --logHeapUsage --config jest.config.js --verbose",
		"docs": "./node_modules/.bin/jsdoc -c ./jsdoc.json -r ./ -d docs"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/plugin-proposal-class-properties": "^7.3.4",
		"@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.5.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.3.4",
		"@typescript-eslint/eslint-plugin": "^3.4.0",
		"@typescript-eslint/parser": "^3.4.0",
		"babel": "^6.23.0",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^11.0.0-beta.0",
		"babel-loader": "^8.0.5",
		"clean-jsdoc-theme": "^2.2.11",
		"eslint": "^7.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.3.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^5.0.0",
		"jest": "^24.8.0",
		"jest-extended": "^0.11.5",
		"jsdoc": "^3.6.2",
		"nodemon": "^1.19.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.5",
		"tui-jsdoc-template": "^1.2.2"
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"@hapi/joi": "^17.1.1",
		"@tensorflow-models/toxicity": "^1.2.2",
		"@tensorflow/tfjs": "^1.5.1",
		"@types/hapi__joi": "^17.1.3",
		"@types/jest": "^26.0.0",
		"@types/mongodb": "^3.5.25",
		"@types/node": "^14.0.14",
		"@types/puppeteer": "^3.0.0",
		"@types/redis": "^2.8.22",
		"chrono-node": "^2.3.0",
		"discord.js": "^12.5.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongodb": "^3.2.7",
		"node-html-to-image": "^3.1.0",
		"puppeteer-cluster": "^0.22.0",
		"redis": "^3.0.2",
		"typescript": "^4.3.5"
	},
	"engines": {
		"node": "14.x"
	}
}
